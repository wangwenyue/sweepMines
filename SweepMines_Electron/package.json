{
  "name": "SweepMines",
  "version": "1.0.0",
  "description": "一个简单的扫雷程序",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:mac": "electron-packager ./ SweepMines --platform=darwin --out=./dist --arch=all --electron-version=2.0.0 --overwrite",
    "build:win64": "electron-packager ./ SweepMines --platform=win32 --out=./dist --arch=x64 --electron-version=2.0.0 --overwrite",
    "build:win32": "electron-packager ./ SweepMines --platform=win32 --out=./dist --arch=ia32 --electron-version=2.0.0 --overwrite"
  },
  "author": "Kowalski",
  "license": "MIT",
  "devDependencies": {
    "electron": "^2.0.0"
  },
  "dependencies": {
    "electron-packager": "^12.1.0"
  }
}
